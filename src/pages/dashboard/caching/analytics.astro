---
import DashboardLayout from '../../../layouts/DashboardLayout.astro';
import Sidebar from '../../../components/layout/Sidebar.astro';
import AlertBox from '../../../components/display/AlertBox.astro';
---

<DashboardLayout title="Cache Analytics" description="Detailed caching analytics">
  <div class="module-wrapper">
    <Sidebar />
    <div class="module-content-wrapper">
      <div class="module-content">
        <div class="module-header">
          <h1>Cache Analytics</h1>
          <p>Detailed caching analytics and reports</p>
        </div>

        <AlertBox
          type="info"
          title="Coming Soon"
          message="Cache analytics functionality will be available soon."
        />
      </div>
    </div>
  </div>
</DashboardLayout>
